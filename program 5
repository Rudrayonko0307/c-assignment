#include <stdio.h>
#include <math.h>
int main()
{ int num, root,a,b;
    printf("Enter any number to find square root: ");
    scanf("%d", &num);
    root = sqrt(num);
    printf("enter base number \n");
    scanf("%d",&a);
    printf("enter power number \n");
    scanf("%d",&b);
    int t=a;
    for(int i=1;i<b;i++)
    {int c=a;
    a=a*t;}
    printf("power of %d by %d = %d\n",t,b,a);
    printf("Square root of %d = %d", num, root);
    return 0;
}
